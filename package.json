{
  "name": "deepin-components",
  "version": "0.0.12",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "analyzer": "ng build --stats-json && webpack-bundle-analyzer dist/deepin-components/stats.json",
    "gen": "tsc mingdao.ts && node mingdao.js",
    "wc": "ng build web-components && cat dist/web-components/*.js > dist/web-components/main.js && cat dist/web-components/*.css > dist/web-components/main.css"
  },
  "private": false,
  "files": [
    "dist/web-components/main.css",
    "dist/web-components/main.js"
  ],
  "dependencies": {
    "@angular/animations": "^16.0.2",
    "@angular/common": "^16.0.2",
    "@angular/compiler": "^16.0.2",
    "@angular/core": "^16.0.2",
    "@angular/elements": "16.0.2",
    "@angular/forms": "^16.0.2",
    "@angular/platform-browser": "^16.0.2",
    "@angular/platform-browser-dynamic": "^16.0.2",
    "@angular/router": "^16.0.2",
    "node-fetch": "2.6.1",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.0.2",
    "@angular/cli": "~16.0.2",
    "@angular/compiler-cli": "^16.0.2",
    "@types/jasmine": "~4.0.0",
    "@types/node-fetch": "^2.6.2",
    "dotenv": "^16.0.1",
    "jasmine-core": "~4.1.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ng-packagr": "^16.0.1",
    "typescript": "~4.9.5",
    "webpack-bundle-analyzer": "^4.5.0"
  }
}