{
  "name": "deepin-components",
  "version": "0.0.7",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "analyzer": "ng build --stats-json && webpack-bundle-analyzer dist/deepin-components/stats.json",
    "gen": "tsc mingdao.ts && node mingdao.js",
    "wc": "ng build web-components && cat dist/web-components/*.js > dist/web-components/main.js && cat dist/web-components/*.css > dist/web-components/main.css"
  },
  "private": false,
  "files": [
    "dist/web-components/main.css",
    "dist/web-components/main.js"
  ],
  "dependencies": {
    "@angular/animations": "^14.0.0",
    "@angular/common": "^14.0.0",
    "@angular/compiler": "^14.0.0",
    "@angular/core": "^14.0.0",
    "@angular/elements": "14.0.3",
    "@angular/forms": "^14.0.0",
    "@angular/platform-browser": "^14.0.0",
    "@angular/platform-browser-dynamic": "^14.0.0",
    "@angular/router": "^14.0.0",
    "@rx-angular/cdk": "1.0.0-beta.3",
    "@rx-angular/template": "1.0.0-beta.33",
    "node-fetch": "^3.2.9",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.0.1",
    "@angular/cli": "~14.0.1",
    "@angular/compiler-cli": "^14.0.0",
    "@types/jasmine": "~4.0.0",
    "dotenv": "^16.0.1",
    "jasmine-core": "~4.1.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ng-packagr": "^14.0.0",
    "typescript": "~4.7.2",
    "webpack-bundle-analyzer": "^4.5.0"
  }
}
